{
  "installCommand": "pnpm install",
  "buildCommand": "mkdir -p api && echo 'module.exports=(req,res)=>res.json({status:\"ok\"})' > api/index.js",
  "outputDirectory": ".",
  "rewrites": [
    { "source": "/(.*)", "destination": "/api/index" }
  ],
  "framework": null,
  "env": {
    "NODE_ENV": "production"
  }
}
