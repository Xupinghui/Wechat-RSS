{
  "installCommand": "pnpm install",
  "buildCommand": "chmod +x build-minimal.sh && ./build-minimal.sh",
  "outputDirectory": ".",
  "rewrites": [
    { "source": "/dash/(.*)", "destination": "/client/dash/$1" },
    { "source": "/(.*)", "destination": "/api/index" }
  ],
  "framework": null,
  "env": {
    "NODE_ENV": "production"
  }
} 