{
  "buildCommand": "pnpm install && pnpm run build:server && pnpm run build:web",
  "outputDirectory": "apps/server/dist",
  "installCommand": "pnpm install",
  "framework": "node",
  "rewrites": [
    { "source": "/dash/(.*)", "destination": "/dash/$1" },
    { "source": "/(.*)", "destination": "/api/$1" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 