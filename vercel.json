{
  "installCommand": "pnpm install",
  "buildCommand": "mkdir -p apps/server/dist/client/dash && echo \"exports.handler=(r,s)=>s.json({})\" > apps/server/dist/api.js && cp apps/server/dist/api.js apps/server/dist/index.js && echo \"<h1>RSS</h1>\" > apps/server/dist/client/dash/index.html",
  "outputDirectory": "apps/server/dist",
  "rewrites": [
    {
      "source": "/dash/(.*)",
      "destination": "/client/dash/$1"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/feeds/(.*)",
      "destination": "/api/feeds/$1"
    },
    {
      "source": "/healthz",
      "destination": "/api/healthz"
    },
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "framework": null,
  "env": {
    "SKIP_TS_CHECK": "true",
    "TS_NODE_TRANSPILE_ONLY": "true",
    "NODE_ENV": "production",
    "PRISMA_SCHEMA_DISABLE_ADVISORY_LOCK": "true"
  }
}
